{
  "name": "my-midway-project",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^2.3.0",
    "@midwayjs/core": "^2.3.0",
    "@midwayjs/decorator": "^2.3.0",
    "@midwayjs/sequelize": "^2.13.2",
    "@midwayjs/web": "^2.3.0",
    "egg": "^2.0.0",
    "egg-scripts": "^2.10.0",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.0",
    "sequelize-typescript": "^2.1.0",
    "sqlite3": "^5.0.2",
    "umzug": "^3.0.0-beta.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@midwayjs/cli": "^1.0.0",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.174",
    "@types/node": "14",
    "code-generator-midway2": "^1.0.2",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^6.0.0",
    "husky": "^7.0.2",
    "jest": "^26.4.0",
    "mwts": "^1.0.5",
    "sequelize": "^6.6.5",
    "ts-jest": "^26.2.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "umzug:h": "node database/migrator.js -h",
    "umzug:u": "node database/migrator.js up",
    "umzug:d": "node database/migrator.js down",
    "umzug:c": "node database/migrator.js create",
    "code": "node ./node_modules/code-generator-midway2/dist/index.js",
    "commit": "git add . && git-cz && git push",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "start": "egg-scripts start --daemon --title=my-midway-project --framework=@midwayjs/web",
    "stop": "egg-scripts stop --title=my-midway-project",
    "start_build": "npm run build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "npm run cov",
    "build": "midway-bin build -c",
    "check": "luckyeye"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-add": "lint-staged",
      "pre-push": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "author": "zhanchao.wu@icloud.com",
  "license": "MIT"
}
